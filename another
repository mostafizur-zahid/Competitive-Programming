#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        int a,b;
        cin>>a>>b;
        int array1[a],array2[b],array[a+b];

        for(int i=0;i<a;i++)cin>>array1[i];
        for(int j=0;j<b;j++)cin>>array2[j];
               
               
             for(int i=0;i<a;i++){
            array[i]=array1[i];
            
        }
        for(int i=0;i<b;i++){
            array[a+i]=array2[i];
            
        }
        cout<<"\n";

        for(int i=0;i<a+b-1;i++){
            cout<<array[i]<<" ";
        }


        
    }
    
    
}